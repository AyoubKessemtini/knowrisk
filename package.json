{
  "name": "knowlepsy",
  "version": "0.0.1",
  "author": "Aironas Kulvelis <aironaskul@gmail.com>",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 14'",
    "install:app": "yarn check-env && yarn install && cd ios && pod install --repo-update && cd ..",
    "install:bundle": "cd ios && bundle install && cd ..",
    "lint": "tsc --noEmit && eslint . --quiet",
    "start": "react-native start",
    "start:clean": "react-native start --reset-cache",
    "test": "jest -u --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --coverage",
    "postinstall": "patch-package",
    "clean:cache": "rm -rf $TMPDIR/metro-* && watchman watch-del-all && yarn cache clean",
    "clean:ios": "rm -rf ios/build && cd ios && pod cache clean --all && pod deintegrate && rm -rf ~/Library/Developer/Xcode/DerivedData/* && cd ..",
    "clean:android": "rm -rf android/build && cd android && ./gradlew clean && cd ..",
    "clean:all": "sh ./scripts/clean-all.sh",
    "check-env": "sh ./scripts/check-env.sh",
    "reset": "find . -type dir -name node_modules | xargs rm -rf && rm -rf yarn.lock",
    "prepare": "husky",
    "generate-icons": "npx icon-set-creator create ./assets/images/app-icon.jpeg",
    "generate-bootsplash": "yarn react-native generate-bootsplash ./assets/svg/logo.svg --platforms=android,ios --assets-output=assets --flavor=main --html=index.html",
    "rename-app": "sh ./scripts/rename-app.sh",
    "link-fonts": "npx react-native-asset",
    "madge": "npx madge --circular --extensions ts,tsx src core"
  },
